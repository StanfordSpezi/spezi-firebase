{
  "name": "spezi-firebase",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Stanford Biodesign Digital Health Spezi Firebase Packages",
  "keywords": [
    "Stanford",
    "Firebase",
    "Biodesign"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StanfordSpezi/spezi-firebase.git"
  },
  "private": true,
  "scripts": {
    "dev": "npm --prefix packages/spezi-firebase-cloud-messaging run watch",
    "prebuild": "concurrently \"npm --prefix packages/spezi-firebase-utils run build\" \"npm --prefix packages/spezi-firebase-cloud-messaging run build\"",
    "build": "concurrently \"npm --prefix packages/spezi-firebase-utils run build\" \"npm --prefix packages/spezi-firebase-cloud-messaging run build\"",
    "build:utils": "npm --prefix packages/spezi-firebase-utils run build",
    "build:frn": "npm --prefix packages/spezi-firebase-cloud-messaging run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix & prettier --write .",
    "lint:ci": "npm run prebuild && eslint --output-file eslint_report.json --format json .",
    "lint:strict": "npm run prebuild && eslint . --max-warnings=0",
    "pretest": "npm run build",
    "test": "npm run test:utils && npm run test:remote-notifications",
    "test:remote-notifications": "npm --prefix packages/spezi-firebase-cloud-messaging run test",
    "test:utils": "npm --prefix packages/spezi-firebase-utils run test",
    "docs": "typedoc",
    "docs:ci": "typedoc --out ./out/docs --githubPages true"
  },
  "dependencies": {
    "@stanfordbdhg/spezi-firebase-cloud-messaging": "file:./packages/spezi-firebase-cloud-messaging"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6",
    "@testing-library/react": "^15",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/jest": "^29",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/parser": "^7",
    "concurrently": "^8",
    "eslint": "^8",
    "eslint-config-prettier": "^9",
    "eslint-plugin-import": "^2",
    "eslint-plugin-prettier": "^5",
    "jest": "^29",
    "jest-environment-jsdom": "^29",
    "prettier": "^3",
    "ts-jest": "^29",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25",
    "typescript": "^5"
  }
}
