  "3": {
    "resourceType": "PlanDefinition",
    "id": "example-plan-4",
    "url": "http://example.org/PlanDefinition/sepsis-protocol",
    "identifier": [
      {
        "system": "http://example.org/plan-definitions",
        "value": "SEPSIS-PROT-001"
      }
    ],
    "version": "3.0.0",
    "name": "SepsisManagementProtocol",
    "title": "Sepsis Management Protocol",
    "subtitle": "Emergency sepsis care pathway",
    "type": "eca-rule",
    "status": "active",
    "experimental": false,
    "date": "2024-03-15T08:00:00Z",
    "publisher": "Emergency Medicine Department",
    "description": "Evidence-based protocol for sepsis management",
    "relatedArtifact": [
      {
        "type": "derived-from",
        "display": "Surviving Sepsis Campaign Guidelines",
        "citation": "Rhodes A, et al. Surviving Sepsis Campaign. Crit Care Med. 2017",
        "url": "http://example.org/guidelines/sepsis"
      }
    ],
    "goal": [
      {
        "category": {
          "coding": [
            {
              "system": "http://terminology.hl7.org/CodeSystem/goal-category",
              "code": "safety",
              "display": "Safety"
            }
          ]
        },
        "description": {
          "text": "Administer antibiotics within 1 hour"
        }
      }
    ],
    "action": [
      {
        "prefix": "1",
        "title": "Recognize Sepsis",
        "description": "Identify patients with suspected sepsis",
        "priority": "stat",
        "trigger": [
          {
            "type": "named-event",
            "name": "sepsis-alert"
          }
        ],
        "condition": [
          {
            "kind": "applicability",
            "expression": {
              "language": "text/fhirpath",
              "expression": "Observation.where(code.coding.where(system='http://loinc.org' and code='8310-5')).value > 38"
            }
          }
        ],
        "groupingBehavior": "logical-group",
        "selectionBehavior": "all",
        "requiredBehavior": "must",
        "precheckBehavior": "yes",
        "cardinalityBehavior": "single"
      },
      {
        "prefix": "2",
        "title": "Obtain Blood Cultures",
        "description": "Draw blood cultures before antibiotics",
        "priority": "stat",
        "code": [
          {
            "coding": [
              {
                "system": "http://snomed.info/sct",
                "code": "30088009",
                "display": "Blood culture"
              }
            ]
          }
        ],
        "relatedAction": [
          {
            "actionId": "administer-antibiotics",
            "relationship": "before-start"
          }
        ]
      },
      {
        "prefix": "3",
        "title": "Administer Broad-Spectrum Antibiotics",
        "description": "Give IV antibiotics within 1 hour",
        "priority": "stat",
        "timingDuration": {
          "value": 1,
          "unit": "hour",
          "system": "http://unitsofmeasure.org",
          "code": "h"
        },
        "participant": [
          {
            "type": "practitioner",
            "role": {
              "coding": [
                {
                  "system": "http://snomed.info/sct",
                  "code": "309343006",
                  "display": "Physician"
                }
              ]
            }
          }
        ],
        "definitionCanonical": "http://example.org/ActivityDefinition/antibiotic-order"
      }
    ]
  }
